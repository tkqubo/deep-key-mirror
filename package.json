{
  "name": "deep-key-mirror",
  "version": "0.3.0",
  "description": "Alternative to React's keyMirror",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "rm -rfv dist && touch docs/.nojekyll",
    "build": "npm run build:tsc && npm run lint && npm run docs",
    "build:tsc": "npm run clean && tsc",
    "start": "npm run build",
    "test": "npm run build && npm run test:jest:cov",
    "test:jest": "jest",
    "test:jest:cov": "jest --coverage --force-exit --detectOpenHandles",
    "test:jest:watch": "jest --watch --detectOpenHandles",
    "test:codeclimate": "codeclimate-test-reporter < coverage/lcov.info # not working now",
    "lint": "eslint .",
    "fix": "npm run lint --fix",
    "docs": "typedoc --out docs --exclude '**/index.ts' src/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkqubo/deep-key-mirror.git"
  },
  "keywords": [
    "React",
    "Flux",
    "Redux",
    "TypeScript",
    "JavaScript",
    "es6"
  ],
  "author": "tkqubo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tkqubo/deep-key-mirror/issues"
  },
  "homepage": "https://github.com/tkqubo/deep-key-mirror#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/power-assert": "^1.5.8",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "espower-typescript": "^10.0.0",
    "jest": "^27.5.1",
    "power-assert": "^1.6.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
